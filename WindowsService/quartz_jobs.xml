<?xml version="1.0" encoding="utf-8" ?>

<!-- This file contains job definitions in schema version 2.0 format -->

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <!--定义示例任务1 Job-->
    <job>
      <name>DemoJob</name>
      <group>DeomJobGroup</group>
      <description>Quartz.Net示例任务</description>
      <job-type>WindowsService.DemoJob,WindowsService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <!--定义示例任务1 触发器 每30秒执行一次DemoJob1任务-->
    <trigger>
      <cron>
        <name>DemoJobTrigger</name>
        <group>DeomJobTriggerGroup</group>
        <job-name>DemoJob</job-name>
        <job-group>DeomJobGroup</job-group>
        <cron-expression>0/30 * * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>